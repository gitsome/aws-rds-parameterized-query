{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$comment": "JSON Schema for client SOW",
    "$id": "IClientSow",
    "title": "IClientSow",
    "type": "object",
    "required": [
        "client_id",
        "is_active",
        "sow_name",
        "session_purchased",
        "session_rate",
        "session_type",
        "session_count_per_coachee",
        "start_date",
        "end_date"
    ],
    "properties": {
        "id": {
            "type": "string",
            "maxLength": 200
        },
        "sow_id": {
            "type": "string",
            "maxLength": 100
        },
        "client_id": {
            "type": "integer"
        },
        "is_active": {
            "type": "string",
            "enum": [
                "Y",
                "N"
            ]
        },
        "client_admin_id": {
            "type": "integer"
        },
        "sow_name": {
            "type": "string",
            "maxLength": 100
        },
        "session_type": {
            "type": "string",
            "enum": [
                "EXECUTIVE",
                "NON-EXECUTIVE"
            ]
        },
        "session_purchased": {
            "type": "integer",
            "minimum": 1
        },
        "session_rate": {
            "type": "number",
            "minimum": 1
        },
        "session_count_per_coachee": {
            "type": "integer",
            "minimum": 1
        },
        "start_date": {
            "type": "string",
            "pattern": "(\\d{4})-(\\d{2})-(\\d{2}) (\\d{2}):(\\d{2}):(\\d{2})"
        },
        "end_date": {
            "type": "string",
            "pattern": "(\\d{4})-(\\d{2})-(\\d{2}) (\\d{2}):(\\d{2}):(\\d{2})"
        }
    }
}
